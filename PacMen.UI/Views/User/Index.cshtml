<p class="Score">
<span> Score: </span><span id="scores"> 0</span> 
<span> Level: </span><span id="LevelEL"> 1</span>
</p>


<div class="cContainer"> 
    <div class="relative">
        <div class="absolute inset-0 flex flex-col items-center justify-center">
            <div id="startScreen" class="bg-black/75 p-4 border-4 border-yellow-400 rounded text-center max-w-xs ">
                <h1 class="text-white text-3xl mb-2">PacMan</h1>
                <button id="startButton"
                        class="text-black text-sm bg-yellow-400 px-4 py-2 rounded-full hover:bg-yellow-300">
                    Start Game
                </button>
            </div>
            <input type="hidden" id="edit-id"> </input>
                <input type="hidden" id="edit-date"></input>
       <!-- game over screen -->
            <div id="gameOverScreen" class="bg-black/75 p-4 border-4 border-yellow-400 rounded text-center max-w-xs w-full hidden">

                <p class="text-white text-xs mb-2">Game Over</p>
                <h1  id="gameOverScoreLabel" class="text-white text-3xl">Score</h1>
                <p class="text-white text-[10px] mb-2">Score</p>
                <h1  id="gameOverScoreLabel2" class="text-white text-3xl">Level</h1>
                <p class="text-white text-[10px] mb-2">Level</p>
                <buttorn id="restartGameButton"
                        class="text-black text-xs bg-yellow-400 px-4 py-2 rounded-full hover:bg-yellow-300">
                    Restart Game
                </button>
            </div>
      <!-- ready tag -->
            <div id="readyTag" class="bg-black/75 p-4 border-4 border-yellow-400 rounded text-center max-w-xs w-full hidden">
                <h1 class="text-white text-xl">Ready?</h1>
            </div>
  <!-- go tag -->
            <div id="goTag" class="bg-black/75 p-4 border-4 border-yellow-400 rounded text-center max-w-xs w-full hidden">
                <h1 class="text-white text-xl">Go!</h1>
            </div>
        </div>
    </div>
</div>


<div class="bContainer">
<canvas class="board" ></canvas>

